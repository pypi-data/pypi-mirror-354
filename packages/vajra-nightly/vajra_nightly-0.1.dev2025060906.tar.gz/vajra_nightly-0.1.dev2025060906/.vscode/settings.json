{
    "search.exclude": {
        "**/third_party": true,
        "**/build": true,
    },
    "cSpell.words": [
        "dedupe",
        "Flashinfer",
        "huggingface",
        "indptr",
        "layernorm",
        "Prioritizer",
        "pyobj",
        "PYPI",
        "replicaset",
        "vajra"
    ],
    "python.testing.pytestArgs": [
        "test"
    ],
    "python.testing.unittestEnabled": false,
    "python.testing.pytestEnabled": true,
    "files.associations": {
        "optional": "cpp",
        "*.ipp": "cpp",
        "any": "cpp",
        "expected": "cpp",
        "format": "cpp",
        "ranges": "cpp",
        "future": "cpp"
    },
    "python.languageServer": "None"
}
