{
	"compilerOptions": {
		"allowJs": true,
		"checkJs": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"sourceMap": true,
		"moduleResolution": "node",
		"target": "esnext",
		"verbatimModuleSyntax": true,
		"noEmit": true
	},
	"include": ["**/*.js"],
	"exclude": ["node_modules", "dist", "gradio.config.js"]
}
