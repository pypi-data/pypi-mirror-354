	institution = {Cancer Genome Project, Wellcome Trust Sanger Institute, Wellcome Trust Genome Campus, Hinxton, CB10 1SA Cambridge, UK.},
	journal = {Nucleic Acids Res},
	keywords = {Cell Line, Tumor; Data Mining; Databases, Nucleic Acid; Genome, Human; Humans; Mutation; Neoplasms, genetics; User-Computer Interface},
	language = {eng},
	medline-pst = {ppublish},
	number = {Database issue},
	owner = {ltfang},
	pages = {D945--D950},
	pii = {gkq929},
	pmid = {20952405},
