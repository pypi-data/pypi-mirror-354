<configure
    xmlns="http://namespaces.zope.org/zope"
    xmlns:p01="http://namespaces.zope.org/p01"
    i18n_domain="p01">

  <!-- JSON-RPC j01AutoSuggest method for form -->
  <p01:jsonrpc
      for="zope.publisher.interfaces.browser.IBrowserPage"
      class=".jsonrpc.FormAutoSuggest"
      methods="j01AutoSuggest"
      permission="zope.Public"
      />

</configure>
