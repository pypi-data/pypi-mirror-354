{
  "name": "mcp-ngobrol",
  "version": "1.0.0",
  "description": "MCP Ngobrol v1.0.0 - Server MCP yang kuat untuk ngobrol interaktif dengan AI. Fork development by MBPR dengan identitas Indonesia.",
  "main": "src/mcp_ngobrol/__main__.py",
  "scripts": {
    "start": "python -m mcp_ngobrol",
    "dev": "python run_mcp_simple.py",
    "test": "python -m pytest tests/",
    "build": "uv build",
    "install": "uv pip install -e .",
    "clean": "rm -rf dist/ build/ *.egg-info/",
    "lint": "python -m flake8 src/",
    "format": "python -m black src/",
    "type-check": "python -m mypy src/"
  },
  "keywords": [
    "mcp",
    "ngobrol",
    "ai",
    "chat",
    "interactive",
    "feedback",
    "indonesia",
    "mbpr",
    "assistant",
    "development",
    "tools"
  ],
  "author": "MBPR",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/MBPR-dev/mcp-ngobrol.git"
  },
  "bugs": {
    "url": "https://github.com/MBPR-dev/mcp-ngobrol/issues"
  },
  "homepage": "https://github.com/MBPR-dev/mcp-ngobrol#readme",
  "engines": {
    "python": ">=3.8"
  },
  "dependencies": {
    "fastmcp": ">=2.0.0",
    "psutil": ">=7.0.0",
    "pyside6": ">=6.8.2.1",
    "fastapi": ">=0.115.0",
    "uvicorn": ">=0.30.0",
    "jinja2": ">=3.1.0",
    "websockets": ">=13.0.0",
    "aiohttp": ">=3.8.0"
  },
  "devDependencies": {
    "pytest": ">=7.0.0",
    "pytest-asyncio": ">=0.21.0",
    "black": ">=23.0.0",
    "flake8": ">=6.0.0",
    "mypy": ">=1.0.0"
  }
