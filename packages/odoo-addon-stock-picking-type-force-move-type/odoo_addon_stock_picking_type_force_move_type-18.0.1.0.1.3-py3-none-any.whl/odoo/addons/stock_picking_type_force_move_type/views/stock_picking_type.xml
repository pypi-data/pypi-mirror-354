<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record model="ir.ui.view" id="view_picking_type_form_inherit">
        <field name="name">Operation Types inherit</field>
        <field name="model">stock.picking.type</field>
        <field name="inherit_id" ref="stock.view_picking_type_form" />
        <field name="arch" type="xml">
            <field name="move_type" position="attributes">
                <!-- Always display 'move_type' std field
                (displayed only on internal transfers by default) -->
                <attribute name="invisible" />
            </field>
            <field name="move_type" position="after">
                <field name="force_move_type" />
            </field>
        </field>
    </record>
</odoo>
