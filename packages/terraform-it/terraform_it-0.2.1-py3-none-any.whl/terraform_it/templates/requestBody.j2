func (r *{{resource_name}}ResourceModel) ToShared{{create_requestBody}}Request() *models.{{ create_requestBody }} {
	{% for reqvar in create_requestmethod -%}
	{{ reqvar }}
	{% endfor %}
	out := models.{{ create_requestBody }}{
		{% for property in create_req_properties -%}
        {{ property | capitalize }}: {{ property }},
        {% endfor %}
	}
	return &out
}

